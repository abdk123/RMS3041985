

   @{
    ViewBag.Title = "Add New User";
    Layout = "~/Views/Shared/_LayoutDlg.cshtml";
}

 
   <div style="width:100%;height:100%;" class="panel panel-primary" iscontainer="true">
                         
                          <div  class="panel-content">
                             <table class="table table-bordered table-striped">
                                 <tr>
                                     <td>
                                        User Name
                                     </td>
                                     <td>
                                          <input class="form-control" id="user_name" bindto="user_name" type="text">
                                     </td>
                                     <td>
                                         Password
                                     </td>
                                     <td>
                                          <input class="form-control" id="password" bindto="password" type="password">
                                        
                                     </td>
                                 </tr>
                                    <tr>
                                     <td>
                                      Name
                                     </td>
                                     <td>
                                         <input class="form-control" id="name" bindto="name" type="text">
                                     </td>
                                     <td>
                                        Surname
                                     </td>
                                     <td>
                                         <input class="form-control" id="sur_name" bindto="sur_name" type="text">
                                     </td>
                                 </tr>
                                  
                                    <tr>
                                     <td>
                                     Department
                                     </td>
                                     <td>
                                          <input  id="Sys_Dept" bindto="Dept" type="text"   >
                                     </td>
                                     <td>
                                        Is Department Admin
                                     </td>
                                     <td>
                                         <input class="form-control" id="IsDeptAdmin" bindto="IsDeptAdmin" type="checkbox">
                                     </td>
                                 </tr>
                                   <tr>
                                     <td>
                                     RO Name
                                     </td>
                                     <td>
                                          <input  id="RO_Name" bindto="RO_Name" type="text" >
                                     </td>
                                     <td>
                                       RO Code
                                     </td>
                                     <td>
                                          <input  id="RO_Code" bindto="RO_Code" type="text" disabled="disabled">
                                     </td>
                                 </tr>
                                 <tr>
                                     <td>
                                         User Type :
                                     </td>
                                     <td>
                                         <input id="Typ" bindto="Typ" type="text">
                                     </td>
                                     <td>
                                         Is Active
                                     </td>
                                     <td>
                                         <input class="form-control" id="Activate" bindto="Activate" type="checkbox">
                                     </td>
                                 </tr>
                                  <tr>
                                     
                                  <td colspan="4">
                                      <input id="submit-button" type="submit" class="ui-button ui-widget ui-state-default ui-corner-all" onclick="updateUser(this); return false;" value="Update User" />
                                   </td>
                                 </tr>
                             </table>

                          </div>
                 </div>
            


<script src="~/Scripts/appjs/usermanagement/updateUser.js?v=25"></script>


